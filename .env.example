# API Configuration
REACT_APP_API_URL=http://localhost:3000/api
REACT_APP_SOCKET_URL=http://localhost:3001

# Application Configuration
REACT_APP_NAME="Parking Management System"
REACT_APP_COMPANY_NAME="Your Company Name"
REACT_APP_LOGO_URL="/logo.png"

# Printer Configuration
REACT_APP_PRINTER_NAME="TM-T88V"
REACT_APP_PRINTER_TYPE="USB"
REACT_APP_PRINTER_WIDTH=80
REACT_APP_PRINTER_DPI=200

# Camera Configuration
REACT_APP_CAMERA_URL="rtsp://camera_ip:554/stream1"
REACT_APP_CAMERA_USERNAME="admin"
REACT_APP_CAMERA_PASSWORD="password"

# Feature Flags
REACT_APP_ENABLE_OFFLINE_MODE=true
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_AUTO_BACKUP=true

# Backup Configuration
REACT_APP_BACKUP_INTERVAL=3600000 # 1 hour in milliseconds
REACT_APP_MAX_BACKUP_SIZE=100 # in MB

# UI Configuration
REACT_APP_THEME_PRIMARY_COLOR="#1976d2"
REACT_APP_THEME_SECONDARY_COLOR="#dc004e"
REACT_APP_THEME_MODE="light"

# Debug Configuration
REACT_APP_DEBUG_MODE=false
REACT_APP_LOG_LEVEL="error" 